@model OrderhistoryVM
@section CSS{

    <style>.container{
    min-height:500px;
    }
        </style>
}
@foreach (var orderId in Model.OrderHej)
{

    var hejhej = orderId;
    <div class="container">
        <h2>Order: @orderId</h2>

        <ul>
            @foreach (var item in Model.Historik)
            {
                @if (hejhej == item.OrderId)
                {
                    <li>
                        Antal: @item.Antal

                        Artikelnamn: @item.ArtikelNamn

                        Pris: @Model.getPriceFormatted(@item.Pris)
                    </li>
                }
            }
        </ul>
    </div>
}